{
  "welcomeFile": "/app/",
  "authenticationMethod": "route",
  "logout": {
    "logoutEndpoint": "/do/logout",
    "backChannelLogoutEndpoint": "/back-channel-logout",
    "logoutPage": "/logout/"
  },
  "routes": [
    {
      "source": "^/destination/([^/]+)/(.*)$",
      "target": "$2",
      "destination": "$1"
    },
    {
      "source": "^/sap/(.*)$",
      "target": "/sap/$1",
      "destination": "SAP_ABAP_BACKEND"
    },
    {
      "source": "^/user-api(.*)",
      "target": "$1",
      "service": "sap-approuter-userapi"
    },
    {
      "source": "^/app/(.*)$",
      "target": "$1",
      "localDir": "webapp",
      "cacheControl": "no-cache, no-store, must-revalidate"
    },
    {
      "source": "^/logout/(.*)$",
      "target": "$1",
      "localDir": "logout",
      "authenticationType": "none"
    }
  ]
}